{
    "name": "ariel/projects",
    "type": "project",
    "require": {
        "cweagans/composer-patches": "^1.6.0",
        "drupal/adminimal_theme": "^1.4",
        "drupal/back_to_top": "^1.0",
        "drupal/block_class": "^1.0",
        "drupal/chosen": "^2.7",
        "drupal/console": "^1.0.2",
        "drupal/core": "^8.6",
        "drupal/devel": "^2.0",
        "drupal/ds": "^3.3",
        "drupal/entityqueue": "^1.0",
        "drupal/field_collection": "^1.0",
        "drupal/menu_link_attributes": "^1.0",
        "drupal/metatag": "^1.8",
        "drupal/module_filter": "^3.1",
        "drupal/owlcarousel": "^1.0",
        "drupal/pathauto": "^1.4",
        "drupal/promote_disable": "^1.0",
        "drupal/redirect": "^1.3",
        "drupal/shs": "^1.0",
        "drupal/superfish": "^1.3",
        "drupal/taxonomy_menu": "^3.4",
        "drupal/taxonomy_menu_ui": "^1.0",
        "drupal/twig_vardumper": "^1.1",
        "drupal/url_redirect": "^2.61",
        "drupal/vardumper": "^1.4",
        "drupal/zurb_foundation": "^6.0",
        "drush/drush": "~8.0|^9.0.0-beta8",
        "hussainweb/drupal-composer-helper": "^1.0",
        "symfony/var-dumper": "~3.4"
    },
    "require-dev": {
        "behat/mink": "~1.7",
        "behat/mink-goutte-driver": "~1.2",
        "jcalderonzumba/gastonjs": "~1.0.2",
        "jcalderonzumba/mink-phantomjs-driver": "~0.3.1",
        "mikey179/vfsstream": "~1.2",
        "phpunit/phpunit": ">=4.8.28 <5",
        "symfony/css-selector": "~2.8"
    },
    "authors": [
        {
            "name": "aacevedoday",
            "email": "you@example.com"
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    ],
    "minimum-stability": "dev",
    "extra": {
        "drupal-composer-helper": {
            "web-prefix": "web"
        },
        "enable-patching": true,
        "installer-paths": {
            "web/core": [
                "type:drupal-core"
            ]
        }
    },
    "prefer-stable": true,
    "conflict": {
        "drupal/drupal": "*"
    },
    "config": {
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
